{
  "name": "voting-app-node",
  "description": "Voting app node project made for freeCodeCamp",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:azdanov/voting-app-node.git",
  "author": "Anton Zdanov <azdanov@protonmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json,css,scss}": ["npm run format", "git add"]
  },
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "npm run clean",
    "start": "node ./dist/index.js",
    "clean": "rimraf ./public ./dist",
    "format": "prettier --write src/**/*.{js,ts,json,css,scss}",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.5",
    "@types/supertest": "^2.0.4",
    "debug": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.3",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.1"
  }
}
